{
  "name": "fond-of-spryker/stock-api",
  "description": "The StockApi module provides a REST API for simple product stock updates.",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcello Muscara",
      "email": "marcello@codeministry.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/api": "^0.3",
    "spryker/api-query-builder": "^0.1",
    "spryker/kernel": "^3.42.4",
    "spryker/stock": "^8.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "codeception/codeception": "^2.5",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
  }
}
